{
    "files.associations": {
        "platform.h": "c",
        "*.x": "c",
        "*.def": "c",
        "*.cfg": "c",
        "limits": "c",
        "vm.h": "c",
        "type_traits": "c",
        "cstdlib": "c",
        "global.h": "c"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/imgs" : true,
        "**/src/arm-trusted-firmware" : true,
        "**/src/buildroot-*" : true,
        "**/src/linux-*" : true,
        "**/src/u-boot" : true,
    },
    "files.watcherExclude": {
        "**/src/arm-trusted-firmware/**": true,
        "**/src/buildroot-*/**": true,
        "**/src/linux-*/**": true,
        "**/src/u-boot/**": true
    }
